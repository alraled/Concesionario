@model IEnumerable<Concesionario.Models.Vehiculo>

@{
    ViewBag.Title = "Index";
}

<h2>LISTADO DE VEHÍCULOS</h2>

<p>
    <button class="btn-default">@Html.ActionLink("Añadir", "Alta")</button>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.matricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.marca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.coste)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.matricula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.marca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.modelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.coste)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo.nombre)
        </td>
 
    </tr>
}

</table>
